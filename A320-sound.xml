<?xml version="1.0"?>

<PropertyList>

<!-- cockpit x=22.5 y=0 z=0.78   -->

<fx>
    <engine>
        <name>left engine</name>
        <path>Sounds/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0</min>
            <max>2</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
        <name>left n1</name>
        <path>Sounds/starter_loop.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>1.5</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/n1</property>
            <factor>0.1</factor>
            <min>1</min>
            <max>8</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>left n1_2</name>
        <path>Sounds/whine.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/n1</property>
            <factor>0.1</factor>
            <min>0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>left reverse thrust</name>
        <path>Sounds/exhaust_loop.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>2</value>
                </greater-than>
                <greater-than>
                    <property>engines/engine[0]/reverser-pos-norm</property>
                    <value>0</value>
                </greater-than>
            </and>
        </condition>
        <volume>
            <property>engines/engine[0]/n1</property>
            <min>0.0</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>left engine inside</name>
        <path>Sounds/starter_loop.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[0]/n1</property>
                    <value>2</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

    <engine>
        <name>right engine</name>
        <path>Sounds/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>engines/engine[1]/n1</property>
                <value>2</value>
            </greater-than>
        </condition>
        <pitch>
            <property>/engines/engine[0]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
            <max>2.0</max>
        </pitch>
        <volume>
            <property>/engines/engine[1]/thrust-lbs</property>
            <factor><factor>0.000015</factor></factor>
            <min>0</min>
            <max>2</max>
        </volume>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>-9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>500.0</max-dist>
