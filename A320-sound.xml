        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>30.0</max-dist>
    </engine>

    <engine>
        <name>right engine inside</name>
        <path>Sounds/starter_loop.wav</path>
        <mode>looped</mode>
        <condition>
            <and>
                <greater-than>
                    <property>engines/engine[1]/n1</property>
                    <value>2</value>
                </greater-than>
                <property>sim/current-view/internal</property>
            </and>
        </condition>
        <pitch>
            <property>/engines/engine[1]/n1</property>
            <offset>0</offset>
            <factor>0.02</factor>
            <min>0.0</min>
