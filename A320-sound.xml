    <!-- Overspeed warning -->
    <overspeed>
        <name>Overspeed warning</name>
        <path>Sounds/overspeed2.wav</path>
        <mode>looped</mode>
        <condition>
            <greater-than>
                <property>velocities/airspeed-kt</property>
                <value>335</value>
            </greater-than>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <position>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </overspeed>

</fx>

