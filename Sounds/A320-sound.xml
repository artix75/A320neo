			<volume>
				<property>engines/engine[0]/thrust_lb</property>
				<factor>0.000028571</factor>
				<min>0.0</min>
				<max>1</max>
			</volume>
			<position>
				<x>15</x>
				<y>-5.7</y>
				<z>-1.1</z>
