		<retard>
			<name>retard</name>
			<path>Aircraft/A320neo/Sounds/res/retard.wav</path>
			<mode>looped</mode>
			<condition>
				<and>
					<property>sim/current-view/internal</property>
					<less-than>
						<property>/position/altitude-agl-ft</property>
						<value>20</value>
					</less-than>

					<greater-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.0</value>
					</greater-than>
					<greater-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.0</value>
					</greater-than>
					<less-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.6</value>
					</less-than>
					<less-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.6</value>
					</less-than>
					<not>
						<property>/gear/gear[0]/wow</property>
						<property>/gear/gear[1]/wow</property>
						<property>/gear/gear[2]/wow</property>
					</not>
					<less-than>
						<property>velocities/airspeed-kt</property>
						<value>150</value>
					</less-than>
				</and>
			</condition>
			<volume>
				<property>/sim/sound/effects/volume</property>
			</volume>
			<type>avionics</type>
			<position>
				<x>22.5</x>
				<y>0</y>
				<z>0.78</z>
			</position>
		</retard>

		<v1>
			<name>V1</name>
			<path>Aircraft/A320neo/Sounds/res/v1.wav</path>
			<condition>
				<and>
					<property>sim/current-view/internal</property>
					<!-- At v1 airspeed -->
					<greater-than-equals>
						<property>velocities/airspeed-kt</property>
						<property>/instrumentation/fmc/vspeeds/V1</property>
					</greater-than-equals>
					<!-- While on ground -->
				Now there is 3D Sound and that you can hear other aircraft. 	<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<!-- With throttles in [flex]Take Off  -->
					<greater-than>
						<property>controls/engines/engine[0]/throttle</property>
						<value>0.7</value>
					</greater-than>
					<greater-than>
						<property>controls/engines/engine[1]/throttle</property>
						<value>0.7</value>
					</greater-than>
				</and>
			</condition>
			<type>avionics</type>
			<volume>
				<property>/sim/sound/effects/volume</property>
				<factor>0.8</factor>
			</volume>
			<position>
				<x>22.5</x>
				<y>0</y>
				<z>0.78</z>
			</position>
		</v1>
	


  <!-- Seatbelt/No smoking signs -->
		<seatbelt>
			<name>Seatbelt sign</name>
			<path>Aircraft/A320neo/Sounds/res/cabinalert.wav</path>
			<mode>once</mode>
			<condition>
				<or>
					<property>sim/sound/no-smoking-sign</property>
					<property>sim/sound/seatbelt-sign</property>
				</or>
				<property>sim/current-view/internal</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<position>
				<x>0.00</x>
				<y>0.00</y>
				<z>0.00</z>
			</position>
			<reference-dist>10.0</reference-dist>
			<max-dist>50.0</max-dist>
		</seatbelt>

	
		<!-- Overspeed warning -->
		<overspeed>
			<name>Overspeed warning</name>
			<path>Sounds/overspeed2.wav</path>
			<mode>looped</mode>
			<type>avionics</type>
			<condition>
				<greater-than>
					<property>velocities/airspeed-kt</property>
					<value>350</value>
				</greater-than>
				<property>sim/current-view/internal</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
			</volume>
			<reference-dist>10</reference-dist>
			<max-dist>100</max-dist>
		</overspeed>

		<stall>
			<name>stall</name>
			<mode>looped</mode>
			<path>/Aircraft/A320neo/Sounds/res/stall.wav</path>
			<type>avionics</type>
			<condition>
				<property>sim/sound/stall-horn</property>
			</condition>
			<volume>
				<property>sim/sound/effects/volume</property>
				<!--offset>1</offset-->
			</volume>
			
			<reference-dist>10</reference-dist>
			<max-dist>100.0</max-dist>
		</stall>
		<autopilotoff>
			<name>Autopilot off</name>
			<path>Aircraft/A320neo/Sounds/res/airbus-warning.wav</path>
			<mode>once</mode>
			<type>avionics</type>
			<condition>
				<equals>
					<property>sim/alarms/AP-Disengage</property>
					<value>true</value>
				</equals>
				<property>sim/current-view/internal</property>
			</condition>

			<volume>
				<property>sim/sound/effects/volume</property>
				<!--factor>2</factor-->
			</volume>

			<reference-dist>10</reference-dist>
			<max-dist>100</max-dist>
		</autopilotoff>

    <!-- New Warnings/Sounds -->

    <gearwarn>
        <name>Gear warning</name>
        <path>/Aircraft/A320neo/Sounds/res/gearwarn.wav</path>
        <mode>looped</mode>
        <type>avionics</type>
        <condition>
            <less-than-equals>
                <property>position/altitude-agl-ft</property>
                <value>400</value>
            </less-than-equals>
            <less-than-equals>
                <property>/velocities/vertical-speed-fps</property>
                <value>-1</value>
            </less-than-equals>
            <not>
                <property>/controls/gear/gear-down</property>
            </not>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </gearwarn>

    <ap_disc>
        <name>AP Disconnect warning</name>
        <path>/Aircraft/A320neo/Sounds/res/airbus-warning.wav</path>
        <mode>once</mode>
        <type>avionics</type>
        <condition>
            <property>/sim/sound/warnings/ap_disc</property>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </ap_disc>

    <chime>
        <name>Chime</name>
        <path>/Aircraft/A320neo/Sounds/res/chime.wav</path>
        <mode>once</mode>
        <type>avionics</type>
        <condition>
            <property>/sim/sound/warnings/chime</property>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </chime>

    <crc>
        <name>CRC</name>
        <path>/Aircraft/A320neo/Sounds/res/crc.wav</path>
        <mode>looped</mode>
        <type>avionics</type>
        <condition>
            <property>/warnings/master-warning-light</property>
            <property>/sim/sound/warnings/crc</property>
        </condition>
        <volume>
            <property>sim/sound/effects/volume</property>
        </volume>
        <reference-dist>10</reference-dist>
        <max-dist>100</max-dist>
    </crc>


	</fx>
</PropertyList>
