		<menubar>
			<default>
				<menu n="3">
					<item>
						<name>autopilot</name>
						<label>Autopilot Settings</label>
						<binding>
							<command>nasal</command>
							<script>
							 var autopilot = gui.Dialog.new("sim/gui/dialogs/autopilot/dialog", "Aircraft/A320neo/Systems/autopilot-dlg.xml");
							 autopilot.open();
							</script>
						</binding>
					</item>
				</menu>
				<menu n="5">
                    <item n="1">
                    <label>Airbus Radio Stack</label>
                        <binding>
                            <command>nasal</command>
                            <script>
							 var airbus_rms = gui.Dialog.new("sim/gui/dialogs/airbus-rms/dialog", "Aircraft/A320neo/Systems/radio-dlg.xml");
							 airbus_rms.open();
							</script>
                        </binding>
                    </item>
                </menu>
				<menu n="100">
					
					<enabled type="bool">true</enabled>
					<item>
						<name>autostart</name>
						<label>Autostart</label>
						<binding>
							<command>property-toggle</command>
							<property>sim/model/start-idling</property>
						</binding>
					</item>
					<item>
						<label>Select Livery</label>
