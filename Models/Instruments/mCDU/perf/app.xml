	<!-- Select Animations -->

	<animation>
		<type>select</type>
		<!--object-name>managed-prompt</object-name-->
		<object-name>title-white</object-name>
		<condition>
			<or>
				<equals>
					<property>flight-management/phase</property>
					<value>T/O</value>
				</equals>
				<equals>
					<property>flight-management/phase</property>
					<value>CLB</value>
				</equals>
				<equals>
					<property>flight-management/phase</property>
					<value>CRZ</value>
				</equals>
				<equals>
					<property>flight-management/phase</property>
					<value>DES</value>
				</equals>
			</or>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>title-green</object-name>
		<condition>
			<equals>
				<property>flight-management/phase</property>
				<value>APP</value>
			</equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>qnh-placeholder</object-name>
		<condition>
			<not>
				<property>/instrumentation/fmc/appr-qnh</property>
			</not>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>qnh-hp</object-name>
		<object-name>qnh-inhg</object-name>
		<condition>
			<property>/instrumentation/fmc/appr-qnh</property>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>qnh-hp</object-name>
		<condition>
			<greater-than>
				<property>/instrumentation/fmc/appr-qnh</property>
				<value>900</value>
			</greater-than>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>qnh-inhg</object-name>
		<condition>
			<less-than>
				<property>/instrumentation/fmc/appr-qnh</property>
				<value>100</value>
			</less-than>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>vapp-placeholder</object-name>
		<condition>
			<and>
				<not>
					<property>/flight-management/spd-manager/approach/app-spd</property>
				</not>
				<not>
					<equals>
						<property>flight-management/phase</property>
						<value>APP</value>
					</equals>
				</not>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>vapp-selected</object-name>
		<condition>
			<property>/flight-management/spd-manager/approach/app-spd</property>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>vapp-managed</object-name>
		<condition>
			<and>
				<not>
					<property>/flight-management/spd-manager/approach/app-spd</property>
				</not>
				<or>
					<equals>
						<property>flight-management/phase</property>
						<value>APP</value>
					</equals>
					<property>/flight-management/fmgc-values/vapp</property>
				</or>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>magwind</object-name>
		<condition>
			<property>/flight-management/settings/mag-wind</property>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>magwind-placeholder</object-name>
		<condition>
			<not>
				<property>/flight-management/settings/mag-wind</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>ldg-conf-full</object-name>
		<condition>
			<greater-than-equals>
				<property>/flight-management/settings/ldg-conf-flaps</property>
				<value>4</value>
			</greater-than-equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>ldg-conf-full-inactive</object-name>
		<condition>
			<less-than>
				<property>/flight-management/settings/ldg-conf-flaps</property>
				<value>4</value>
			</less-than>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>ldg-conf3</object-name>
		<condition>
			<less-than>
				<property>/flight-management/settings/ldg-conf-flaps</property>
				<value>4</value>
			</less-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>ldg-conf3-inactive</object-name>
		<condition>
			<greater-than-equals>
				<property>/flight-management/settings/ldg-conf-flaps</property>
				<value>4</value>
			</greater-than-equals>
		</condition>
	</animation>
	
	<!-- Button Pick Animations -->

	<animation>
		<type>pick</type>
		<object-name>l1</object-name>
		<action>
			<button>0</button>
			   <repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				 <script>
					 var input = getprop("/instrumentation/mcdu/input");
					 if(input == nil or input == '') input = 0;
					 var qnh = num(input);
					 if(qnh != nil){
					 	if(qnh > 100) qnh = int(qnh);
					 	setprop('/instrumentation/fmc/appr-qnh', qnh);
					 }

					 setprop("/instrumentation/mcdu/input", "");
				</script>
			</binding>
		</action>
	</animation>
    
	<animation>
		<type>pick</type>
		<object-name>l3</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>

					var input = getprop("/instrumentation/mcdu/input");
					var wind_comp = split('/', input);
					if(size(wind_comp) == 2){
						var wnd_dir = num(string.trim(wind_comp[0]));
						var wnd_kt = num(string.trim(wind_comp[1]));
						if(wnd_dir != nil and wnd_kt != nil)
							setprop('/flight-management/settings/mag-wind', input);
					}
					setprop("/instrumentation/mcdu/input", "");

				</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>l4</object-name>
		<action>
			<button>0</button>
			   <repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				 <script>
				 
					var input = getprop("/instrumentation/mcdu/input");
					var trans_alt = num(input);
					if(trans_alt != nil){
						setprop("/instrumentation/fmc/appr-trans-alt", trans_alt);
						var phase = getprop('flight-management/phase');
						if(phase != 'T/O' and phase != 'CLB' and phase != 'CRZ')
							setprop('flight-management/fmgc-values/trans-alt', trans_alt);
					}
					 #setprop("/flight-management/spd-manager/approach/app-spd", input);
					 
					setprop("/instrumentation/mcdu/input", "");
					 
				</script>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>l5</object-name>
		<action>
			<button>0</button>
			   <repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				 <script>
				 
					 var input = getprop("/instrumentation/mcdu/input");
					 
