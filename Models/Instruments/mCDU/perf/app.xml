		<object-name>vapp-selected</object-name>
		<object-name>magwind</object-name>
		<object-name>ldg-conf3</object-name>
		<object-name>ldg-conf3-inactive</object-name>
		<object-name>ldg-conf-full</object-name>
		<object-name>ldg-conf-full-inactive</object-name>
		<emission>
			<red>0</red>
			<green>0.4823</green>
			<blue>0.5372</blue>
			<factor-prop>/instrumentation/mcdu/brt</factor-prop>
		</emission>
